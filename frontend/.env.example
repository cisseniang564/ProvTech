# üìÅ Racine: frontend/
# ‚îî‚îÄ‚îÄ .env.example

# Application
REACT_APP_NAME=ProvTech Actuarial SaaS
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_API_TIMEOUT=30000

# Authentication
REACT_APP_AUTH_DOMAIN=localhost
REACT_APP_TOKEN_REFRESH_INTERVAL=3600000
REACT_APP_SESSION_TIMEOUT=7200000

# Features Flags
REACT_APP_ENABLE_MONTE_CARLO=true
REACT_APP_ENABLE_MACHINE_LEARNING=false
REACT_APP_ENABLE_BENCHMARKING=true
REACT_APP_ENABLE_AUDIT_TRAIL=true
REACT_APP_ENABLE_MULTI_LANGUAGE=false

# Analytics & Monitoring
REACT_APP_SENTRY_DSN=
REACT_APP_GA_TRACKING_ID=
REACT_APP_MIXPANEL_TOKEN=
REACT_APP_ENABLE_ANALYTICS=false

# Storage
REACT_APP_MAX_FILE_SIZE=52428800
REACT_APP_ALLOWED_FILE_TYPES=.csv,.xlsx,.xls,.json
REACT_APP_STORAGE_TYPE=local

# Map Configuration (if using maps)
REACT_APP_MAPBOX_TOKEN=

# Regulatory Compliance
REACT_APP_IFRS17_ENABLED=true
REACT_APP_SOLVENCY2_ENABLED=true
REACT_APP_DEFAULT_CURRENCY=EUR
REACT_APP_DEFAULT_LANGUAGE=fr

# Performance
REACT_APP_ENABLE_CACHE=true
REACT_APP_CACHE_TTL=300000
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_CHUNK_SIZE=1048576

# Security
REACT_APP_ENABLE_ENCRYPTION=true
REACT_APP_CSP_REPORT_URI=
REACT_APP_ENABLE_RATE_LIMITING=true
REACT_APP_MAX_LOGIN_ATTEMPTS=5

# Development
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_DEVTOOLS=false
REACT_APP_MOCK_API=false
REACT_APP_LOG_LEVEL=error